<?xml version="1.0"?>
<cd:Record format="binary" name="SCI_NL__1P_GeoL" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="pos_esm">
    <cd:Float>
      <cd:Description>Position of ESM compared to zero position</cd:Description>
      <cd:Unit>degrees</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="pos_asm">
    <cd:Float>
      <cd:Description>Position of ASM compared to zero position</cd:Description>
      <cd:Unit>degrees</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="sol_zen_ang">
    <cd:Array>
      <cd:Description>Solar zenith angles of start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Float>
        <cd:Unit>degrees</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="sol_azi_ang">
    <cd:Array>
      <cd:Description>Solar azimuth angles of start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Float>
        <cd:Unit>degrees</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="los_zen_ang">
    <cd:Array>
      <cd:Description>Line of sight nadir angles of start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Float>
        <cd:Unit>degrees</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="los_azi_ang">
    <cd:Array>
      <cd:Description>Line of sight azimuth angles of start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Float>
        <cd:Unit>degrees</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="sat_h">
    <cd:Float>
      <cd:Description>Satellite height at the middle of the integration time</cd:Description>
      <cd:Unit>km</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="earth_rad">
    <cd:Float>
      <cd:Description>Earth radius at the middle of the integration time</cd:Description>
      <cd:Unit>km</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="sub_sat_point">
    <cd:Record>
      <cd:Description>Sub-satellite point at the middle of the integration time</cd:Description>
      <cd:Field name="latitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_north</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_north</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
      <cd:Field name="longitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_east</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_east</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="tang_ground_point">
    <cd:Array>
      <cd:Description>Coordinates of tangent ground point of the start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Record>
        <cd:Field name="latitude">
          <cd:Integer>
            <cd:Unit>1e-6 degrees_north</cd:Unit>
            <cd:Conversion numerator="1.000000" denominator="1000000.000000">
              <cd:Unit>degrees_north</cd:Unit>
            </cd:Conversion>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>int32</cd:NativeType>
          </cd:Integer>
        </cd:Field>
        <cd:Field name="longitude">
          <cd:Integer>
            <cd:Unit>1e-6 degrees_east</cd:Unit>
            <cd:Conversion numerator="1.000000" denominator="1000000.000000">
              <cd:Unit>degrees_east</cd:Unit>
            </cd:Conversion>
            <cd:BitSize>32</cd:BitSize>
            <cd:NativeType>int32</cd:NativeType>
          </cd:Integer>
        </cd:Field>
      </cd:Record>
    </cd:Array>
  </cd:Field>
  <cd:Field name="tan_h">
    <cd:Array>
      <cd:Description>Tangent height of the start, middle, and end of the integration time</cd:Description>
      <cd:Dimension>3</cd:Dimension>
      <cd:Float>
        <cd:Unit>km</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="dopp_shift">
    <cd:Float>
      <cd:Description>Doppler shift at 500nm at the middle of the integration time</cd:Description>
      <cd:Unit>nm</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
</cd:Record>
