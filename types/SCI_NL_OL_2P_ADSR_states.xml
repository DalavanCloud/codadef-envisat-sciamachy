<?xml version="1.0"?>
<cd:Record format="binary" name="SCI_NL_OL_2P_ADSR_states" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start time of the scan phase of the state</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment Flag (set to 1 if all MDSRs corresponding to this ADSR are blank, set to zero otherwise)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="state_id">
    <cd:Integer>
      <cd:Description>State ID</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="duration_scan_state">
    <cd:Integer>
      <cd:Description>Duration of scan phase of the state</cd:Description>
      <cd:Unit>1/16 s</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="16.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="longest_int_time">
    <cd:Integer>
      <cd:Description>Longest integration time</cd:Description>
      <cd:Unit>1/16 s</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="16.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="shortest_int_time">
    <cd:Integer>
      <cd:Description>Shortest integration time</cd:Description>
      <cd:Unit>1/16 s</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="16.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="num_obs_state">
    <cd:Integer>
      <cd:Description>Number of geolocation records for this state</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
</cd:Record>
