<?xml version="1.0"?>
<cd:Record format="binary" name="SCI_RV__2P_MDSR_ver_col_o3" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start time of the DSR</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="quality_flag">
    <cd:Integer>
      <cd:Description>Quality Indicator (-1 for blank MDSR, 0 otherwise)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>int8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="integr_time">
    <cd:Integer>
      <cd:Description>Integration time for this DSR</cd:Description>
      <cd:Unit>1/16 s</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="16.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="o3_vcd">
    <cd:Float>
      <cd:Description>Ozone Vertical Column Density</cd:Description>
      <cd:Unit>molecules/cm2</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="err_vcd">
    <cd:Float>
      <cd:Description>Error on the VCD above</cd:Description>
      <cd:Unit>%</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
</cd:Record>
