        </cd:Field>
      </cd:Record>
    </cd:Array>
  </cd:Field>
  <cd:Field name="wvlen_grid_spec_seg_type_3">
    <cd:Array>
      <cd:Description>Wavelength grid per spectral segment</cd:Description>
      <cd:Dimension><![CDATA[if(int(../type_wvlen_info) == 3, int(../num_spec_seg), 0)]]></cd:Dimension>
