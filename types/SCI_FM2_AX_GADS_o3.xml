      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="num_spec_entries">
    <cd:Array>
      <cd:Description>Number of spectral entries per segment</cd:Description>
