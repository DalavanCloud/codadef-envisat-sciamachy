        <cd:Dimension><![CDATA[int(../../num_spec_entries[index(:)])]]></cd:Dimension>
        <cd:Float>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>float</cd:NativeType>
        </cd:Float>
      </cd:Array>
    </cd:Array>
