<?xml version="1.0"?>
<cd:Record format="binary" name="SCI_NL__1P_GeoCal" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="pos_esm">
    <cd:Float>
      <cd:Description>Position of ESM compared to zero position</cd:Description>
      <cd:Unit>degrees</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="pos_asm">
    <cd:Float>
      <cd:Description>Position of ASM compared to zero position</cd:Description>
      <cd:Unit>degrees</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="sol_zen_ang">
    <cd:Float>
      <cd:Description>Solar zenith angle at the middle of the integration time</cd:Description>
      <cd:Unit>degrees</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
  <cd:Field name="sub_sat_point">
    <cd:Record>
      <cd:Description>Sub-satellite point at the middle of the integration time</cd:Description>
      <cd:Field name="latitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_north</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_north</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
      <cd:Field name="longitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_east</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_east</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
    </cd:Record>
  </cd:Field>
</cd:Record>
